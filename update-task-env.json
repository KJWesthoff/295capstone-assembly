{
  "family": "ventiapi-backend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::712155057496:role/ventiapi-task-execution-role",
  "taskRoleArn": "arn:aws:iam::712155057496:role/ventiapi-task-role",
  "containerDefinitions": [
    {
      "name": "ventiapi-backend",
      "image": "712155057496.dkr.ecr.us-west-1.amazonaws.com/ventiapi-scanner/backend:latest",
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "PYTHONUNBUFFERED",
          "value": "1"
        },
        {
          "name": "SCANNER_MAX_PARALLEL_CONTAINERS",
          "value": "5"
        },
        {
          "name": "REDIS_URL",
          "value": "redis://ventiapi-scanner-redis.rn2squ.0001.usw1.cache.amazonaws.com:6379"
        },
        {
          "name": "SCANNER_CONTAINER_MEMORY_LIMIT",
          "value": "512m"
        },
        {
          "name": "USE_AWS_SCANNER",
          "value": "true"
        },
        {
          "name": "ECS_CLUSTER_NAME",
          "value": "ventiapi-scanner-cluster"
        },
        {
          "name": "SCANNER_TASK_DEFINITION",
          "value": "ventiapi-scanner:1"
        },
        {
          "name": "SUBNET_IDS",
          "value": "subnet-02fdba6c870945a66,subnet-00234d6198abac831"
        },
        {
          "name": "SECURITY_GROUP_IDS",
          "value": "sg-0483c60d2f8d951fc"
        },
        {
          "name": "AWS_REGION",
          "value": "us-west-1"
        },
        {
          "name": "DEFAULT_ADMIN_USERNAME",
          "value": "MICS295"
        },
        {
          "name": "DEFAULT_ADMIN_PASSWORD",
          "value": "MaryMcHale"
        },
        {
          "name": "S3_BUCKET",
          "value": "ventiapi-scanner-storage-712155057496"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ventiapi-backend",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}